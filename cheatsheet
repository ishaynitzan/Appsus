function formatted_date(date)
{
   var d = new Date(date);
   var result="";
   result += d.getFullYear()+"/"+(d.getMonth()+1)+"/"+d.getDate() + 
             " "+ d.getHours()+":"+d.getMinutes()+":"+
             d.getSeconds()+" "+d.getMilliseconds();
   return result;
}

formatted_date(val) {
      var result = "";
      var date = "" + new Date(val);
      date = date.split(" ");
      result += date[1] + "/" + date[2];
      return result;
